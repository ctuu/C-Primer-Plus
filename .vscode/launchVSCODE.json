{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${file}.o",
            "externalConsole": true,
            "preLaunchTask": "gcc",
            "cwd": "${workspaceRoot}",
            "linux": {
                "MIMode": "gdb"
            }
        },

        {
            "name": "C Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "${file}.o",
            "processId": "${command.pickProcess}",
            "linux": {
                "MIMode": "gdb"
            }
        }
    ]
}