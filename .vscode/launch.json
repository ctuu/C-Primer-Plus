{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug",
            "type": "gdb",
            "request": "launch",
            "target": "${file}.o",
            "cwd": "${workspaceRoot}",
            "terminal": "",
            "preLaunchTask": "gcc",
            "windows": {
              "miDebuggerPath": "c:\\MinGW\\bin\\gdb.exe"  
            }
        },
        {
            "name": "C Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${file}.o",
            "externalConsole": true,
            "args": [],
            "preLaunchTask": "gcc",
            "cwd": "${workspaceRoot}",
            "logging": {
                "programOutput": false,
                "moduleLoad": false
            },
            "linux": {
                "MIMode": "gdb"
            },
            "windows": {
              "miDebuggerPath": "c:\\MinGW\\bin\\gdb.exe"  
            }
        },
        {
            "name": "C Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "${file}.o",
            "processId": "${command.pickProcess}",
            "linux": {
                "MIMode": "gdb"
            }
        }
    ]
}